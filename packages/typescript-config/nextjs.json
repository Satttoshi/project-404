{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "jsx": "preserve",
    "noEmit": true
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.mjs", "next-env.d.ts", "next.config.mjs", ".next/types/**/*.ts"]
}
